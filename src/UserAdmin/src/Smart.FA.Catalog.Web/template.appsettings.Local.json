{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "DetailedErrors": true,
  "ConnectionStrings": {
    "Catalog": "Server=host.docker.internal; Database=Catalog; User Id =SA; Password=localStoragePassword!"
  },
  "ForceHttpRedirection": false,
  "MailOptions": {
    "Server": "",
    "Sender": ""
  },
  "AdminOptions": {
    "Training": {
      "NumberOfTrainingsDisplayed": 3
    }
  },
  "S3Storage": {
    "ImageBucketName": "cfa",
    "FileSizeLimit": 2097152,
    "Credentials": {
      "SecretKey": "newuser123",
      "AccessKey": "newuser"
    },
    "AWS": {
      "RegionEndpoint": "us-east-1",
      "ServiceUrl": "http://host.docker.internal:9000",
      "ForcePathStyle": true
    }
  },
  "MediatR": {
    "LogRequests": true
  },
  "EFCore": {
    "UseConsoleLogger": true
  },
  "Authentication": {
    "UseFakeHeaders": true
  },
  "SuperUser": {
    "NumberOfTrainingsPerPage": 2,
    "NumberOfTrainersPerPage": 4
  }
}
