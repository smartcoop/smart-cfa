{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "DetailedErrors": true,
  "ConnectionStrings": {
    "Catalog": "Server={catalog-server-name},1433;Database=Catalog;User Id ={catalog-server-user-id}; Password={catalog-server-user-password}",
    "Account": "Server={account-server-name},1433; Database=Account; User Id ={account-server-user-id}; Password={account-server-user-password}"
  },
  "MailOptions": {
    "Server": "",
    "Sender": ""
  },
  "AdminOptions": {
    "Training": {
      "NumberOfTrainingsDisplayed": 3
    }
  },
  "S3Storage": {
    "ImageBucketName": "cfa",
    "FileSizeLimit": 2097152,
    "Credentials": {
      "SecretKey": "newuser123",
      "AccessKey": "newuser"
    },
    "AWS": {
      "RegionEndpoint": "us-east-1",
      "ServiceUrl": "http://{docker_name-minio}:9000",
      "ForcePathStyle": true
    }
  },
  "EFCore": {
    "UseConsoleLogger": false
  }
}
